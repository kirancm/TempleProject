extends ../layouts/default

block content
	.container: .row: .col-sm-10.col-sm-offset-1.col-md-8.col-md-offset-2
		article
			if !data.committees
				h2 Invalid committee.
			else
				header
					h2= data.committees.title
					!= data.committees.description
					.committeeMembers
						each member in data.committees.members
							div
								h4= member	
					!= data.committees.activities